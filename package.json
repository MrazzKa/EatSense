{
  "name": "eatsense-mobile",
  "version": "1.0.0",
  "description": "EatSense Mobile Client - React Native + Expo",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env -- expo start --tunnel --clear",
    "start:tunnel": "dotenv -e .env -- expo start --tunnel --clear",
    "start:lan": "dotenv -e .env -- expo start --lan --clear",
    "start:dev": "REACT_NATIVE_PACKAGER_HOSTNAME=192.168.3.6 dotenv -e .env -- expo start --lan --clear --reset-cache",
    "start:dev:auto": "dotenv -e .env -- expo start --lan --clear",
    "convert-logo": "./scripts/convert-logo.sh",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "npx eas-cli@latest build -p android --profile production",
    "build:ios": "npx eas-cli@latest build -p ios --profile production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "detox test",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint && npm run type-check && npm run test",
    "setup": "pnpm install && pnpm run setup:api",
    "i18n:extract": "node scripts/i18n-extract.js",
    "i18n:verify": "node scripts/i18n-verify.js",
    "build": "nest build",
    "start:prod": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "eas:whoami": "npx eas-cli@latest whoami",
    "eas:login": "npx eas-cli@latest login",
    "eas:build:ios:dev": "npx eas-cli@latest build -p ios --profile development",
    "eas:build:ios": "npx eas-cli@latest build -p ios --profile production --non-interactive",
    "eas:build:ios:local": "npx eas-cli@latest build --platform ios --profile production --local",
    "eas:build:android": "npx eas-cli@latest build -p android --profile production --non-interactive",
    "eas:submit:ios": "npx eas-cli@latest submit -p ios --profile production --latest",
    "check:errors": "bash check-errors.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "7.8.5",
    "@react-navigation/native": "7.1.21",
    "@react-navigation/native-stack": "^7.3.16",
    "@react-navigation/stack": "7.6.4",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "cli-spinners": "^3.3.0",
    "core-js": "^3.40.0",
    "expo": "~54.0.0",
    "expo-apple-authentication": "~8.0.7",
    "expo-application": "~7.0.7",
    "expo-asset": "~12.0.10",
    "expo-auth-session": "~7.0.9",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.18",
    "expo-device": "~8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "^19.0.19",
    "expo-font": "~14.0.9",
    "expo-haptics": "~14.0.0",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.13",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "i18next": "^23.16.8",
    "moti": "^0.28.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-render-html": "^6.3.4",
    "react-native-worklets": "0.5.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.18.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.5.0",
    "@testing-library/react-native": "^12.8.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.13",
    "prettier": "^3.2.5",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": "20.19.x"
  },
  "packageManager": "pnpm@10.19.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "expo": {
    "install": {
      "exclude": [
        "typescript"
      ]
    }
  }
}
