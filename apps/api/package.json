{
  "name": "eatsense-api",
  "version": "1.0.0",
  "description": "EatSense API - AI-powered nutrition analysis backend",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prestart:prod": "prisma generate && prisma migrate deploy",
    "start:railway": "prisma generate && prisma migrate deploy && node dist/main.js",
    "lint": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "setup:env": "node scripts/setup-env.js",
    "fdc:import:foundation": "ts-node -r tsconfig-paths/register src/fdc/etl/etl-import-raw.ts --file=$USDA_DATA_DIR/foundation-2025-04.json --type=Foundation",
    "fdc:import:fndds": "ts-node -r tsconfig-paths/register src/fdc/etl/etl-import-raw.ts --file=$USDA_DATA_DIR/fndds-2021-2023.json --type=FNDDS",
    "fdc:import:sr": "ts-node -r tsconfig-paths/register src/fdc/etl/etl-import-raw.ts --file=$USDA_DATA_DIR/sr-legacy-2018-04.json --type=SRLegacy",
    "fdc:import:branded": "ts-node -r tsconfig-paths/register src/fdc/etl/etl-import-raw.ts --file=$USDA_DATA_DIR/branded-2025-04.json --type=Branded",
    "fdc:reindex": "ts-node -r tsconfig-paths/register src/fdc/etl/etl-reindex.ts",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed:nutrients": "ts-node -r tsconfig-paths/register prisma/seeds/seed-nutrients.ts",
    "prisma:seed:articles": "ts-node -r tsconfig-paths/register prisma/seeds/seed-articles.ts",
    "prisma:seed:local-foods": "ts-node -r tsconfig-paths/register prisma/seeds/seed-local-foods.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@prisma/client": "^6.18.0",
    "@sendgrid/mail": "^8.1.0",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.0.0",
    "bullmq": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "expo-server-sdk": "^3.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "fast-glob": "^3.3.0",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "jose": "^5.2.0",
    "luxon": "^3.4.4",
    "marked": "^12.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "openai": "^4.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.0",
    "redis": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stream-json": "^1.8.0",
    "ts-node": "^10.9.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.23",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/typescript-estree": "^8.49.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
