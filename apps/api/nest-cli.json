{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "root": ".",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.json",
        "outDir": "dist",
        "watchAssets": true
      },
      {
        "include": "assets/**/*",
        "outDir": "dist",
        "watchAssets": true,
        "exclude": "**/*.ts"
      }
    ]
  },
  "test": false
}
